{
  "expo": {
    "name": "T4YC",
    "slug": "moika",
    "version": "1.0.0",
    "scheme": "ru.t4yc.moikamobile",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-notifications"
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ru.t4yc.moikamobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "ru.t4yc.moikamobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
